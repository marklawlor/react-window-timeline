    "react-dom": "17.0.1",
    "react-is": "17.0.1",
    "react-virtualized-auto-sizer": "^1.0.4",
    "size-limit": "4.9.2",
    "tsdx": "0.14.1",
    "tslib": "2.1.0",
    "typescript": "4.2.2",
    "uuid": "8.3.2"
  },
  "dependencies": {
    "react-merge-refs": "^1.1.0",
    "react-window": "1.8.6"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "4.15.2",
