{
  "version": "0.8.7",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marklawlor/react-window-timeline.git"
  },
  "scripts": {
    "start": "tsdx watch --tsconfig ./tsconfig.build.json",
    "build": "tsdx build --tsconfig ./tsconfig.build.json",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postdeploy": "npm run build-storybook && bs --directory storybook-static"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "keywords": [
    "react",
    "timeline",
    "calendar"
  ],
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-window-timeline",
  "author": "Mark Lawlor",
  "module": "dist/react-window-timeline.esm.js",
  "size-limit": [
    {
      "path": "dist/react-window-timeline.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-window-timeline.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.13.8",
